<%= form_with(model: [ :profile, @project], local: true) do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :technologies %><br>
    <%= tech_field(@project) %>
  </p>
  <p>
    <%= form.label :description %><br>
    <%= form.text_field :description %>
  </p>
  <p>
    <%= form.label :url_link %><br>
    <%= form.text_field :url_link %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>
